  "dependencies": {
    "@ladle/react": "^2.4.5",
    "@mdx-js/mdx": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@mdx-js/rollup": "^2.1.5",
    "@stefanprobst/rehype-extract-toc": "^2.2.0",
    "@vitejs/plugin-react": "^2.2.0",
    "dayjs": "^1.11.6",
    "express": "^4.18.2",
    "fela": "^12.0.2",
    "fela-dom": "^12.0.2",
    "fs-extra": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fela": "^12.0.2",
    "react-head": "^3.4.0",
    "react-syntax-highlighter": "^15.5.0",
